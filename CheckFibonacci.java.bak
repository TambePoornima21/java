import java.util.Scanner;
class CheckFibonacci
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a number : ");
		int num=sc.nextInt();

		int n1=0;
		int n2=1;
		int n3=0;

		for (int i=0;i<=n ;i++ )
		{
			while (n3<num)
			{
				n3=n1+n2;
				n1=n2;
				n2=n3;
			}
			if (n3==num)
			{
				System.out.println("This number belongs to Fibonacci Series");
			}
			else
			{
				System.out.println("Thsi number does not belong to Fibonacci Series");
			}
		}
	}
}
