class Substraction 
{
	public static void main(String[] args) 
	{
		byte b1 = 3;
		byte b2= 2;
		int b3=b1 - b2;
		System.out.println(b3);
	 
          
	 
		short s1 = 10;
		short s2 = 20;
		int s3 = s1 - s2;
		System.out.println(s3);
	 	

	 
		int i1 = 100;
		int i2 = 200;
		int i3= i1 - i2;
		System.out.println(i3);
	 

	 
		long l1 = 500l;
		long l2= 1000l;
		long l3= l1-l2;
		System.out.println(l3);
	 
	
	 
		float f1 = 5.5f;
		float f2= 5.5f;
		float f3= f1 - f2;
		System.out.println(f3);
	 

	 
		double d1 = 10.5;
		double d2= 10.5;
		double d3 = d1 - d2;
		System.out.println(d3);
	 

	 
		char ch1 = 'a';
		char ch2 = 'b';
		int ch3 = ch1 - ch2;
		System.out.println(ch3);
	 

	 
		String s1 = "Maharashtra ";
		String s2 = "Pune";
		String s3 = s1 - s2;
		System.out.println(s3);
	}
}
